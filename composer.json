{
    "name": "torreswil/carbon_colombia",
    "type": "library",
    "description": "Extension de nesbot/carbon, para trabajar con festivos de Colombia",
    "keywords": [
        "torreswil",
        "carbon_colombia"
    ],
    "homepage": "https://github.com/torreswil/carbon_colombia",
    "license": "MIT",
    "authors": [
        {
            "name": "Wilfredo Torres",
            "email": "wtorresariza@gmail.com",
            "homepage": "http://wilto.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.8",
        "nesbot/carbon": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "torreswil\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "torreswil\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
